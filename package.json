{
  "name": "six-degrees-of-zlatan",
  "version": "0.0.1",
  "description": "Using puppeteer to scrape some football data from the interwebs",
  "main": "scrapper.ts",
  "type": "module",
  "scripts": {
    "build": "tsc --outDir dist",
    "scrapper": "tsc --outDir dist && cd dist && node scrapper.js",
    "zlatan": "tsc --outDir dist && cd dist && node zlatan.js"
  },
  "dependencies": {
    "blessed": "^0.1.81",
    "csv": "^6.3.5",
    "csv-stringify": "^6.4.4",
    "figlet": "^1.6.0",
    "flexsearch": "0.7.11",
    "mersennetwister": "^0.2.3",
    "puppeteer": "^21.3.7",
    "readline-sync": "^1.4.10",
    "terminal-kit": "^3.0.1"
  },
  "devDependencies": {
    "@types/flexsearch": "^0.7.4",
    "@types/node": "^20.3.3",
    "@types/terminal-kit": "^2.5.2",
    "puppeteer-debug": "^1.0.1",
    "ts-node": "^8.2.0",
    "tslint": "^5.17.0",
    "typescript": "^4.9.5"
  }
}
